# SSH Configuration for Remote Deployment
SSH_HOST=example.com
SSH_USER=root
DOMAIN=ai-gateway.example.com
SSH_KEY=~/.ssh/id_rsa
SSH_PORT=22

# OTLP telemetry configuration (optional, used when OTLP_ENDPOINT and OTLP_API_KEY are set)
OTLP_ENDPOINT=
OTLP_API_KEY=
# Optional metadata for resource discovery
OTLP_SERVICE_NAME=ai-gateway
OTLP_RESOURCE_ATTRIBUTES=deployment.environment=production
# Additional headers appended to the OTLP exporter (format: Key=Value,Key2=Value2)
OTLP_HEADERS=